{
 "buildCommand": "prisma generate && next build",
 "env": {
 "PRISMA_GENERATE_SKIP_AUTOINSTALL": "true"
 },
 "functions": {
 "app/api/**/*.js": {
 "maxDuration": 60
 },
 "src/app/api/cv/export/[id]/route.ts": {
 "maxDuration": 60,
 "memory": 1024
 }
 },
 "crons": [
   {
     "path": "/api/cron/cancel-expired-subscriptions",
     "schedule": "15 0 * * *"
   },
   {
     "path": "/api/cron/deactivate-expired-promodes",
     "schedule": "30 0 * * *"
   }
 ]
}
